file = {:C$\backslash$:/Users/gsanh/Documents/Mendeley Desktop//Edwin et al. - 2017 - Dise{\~{n}}o {\'{O}}ptimo de Redes de Distribuci{\'{o}}n de Agua Usando Un Software Basado En Microalgoritmos Gen{\'{e}}ticos Multiobjetiv.pdf:pdf},
issn = {2386-3781},
journal = {Ribagua},
keywords = {Agua,algoritmos gen{\'{e}}ticos,multiobjetivo,optimizaci{\'{o}}n,redes de agua},
mendeley-groups = {Memoria,Memoria/Trabajos relacionados,Memoria/Referencias Usadas propuesta,Memoria/Referencias Usadas Doc Final},
